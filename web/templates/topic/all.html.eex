

<div class="row">
<ul  class="list-group">
<li  class="list-group-item active"><h5>Topics</h5>
<%= for topic <- @topics do %>
  <li  class="list-group-item"><%= topic.title %>
  <%= link("Delete", to: topic_path(@conn, :delete, topic), method: :delete, class: "badge badge-secondary") %>
  <%= link("Edit", to: topic_path(@conn, :edit, topic), class: "badge badge-primary") %>
<% end %>
</ul>
</div>
